                    <button class="btn btn-sm btn-danger text-end">Delete</button>
                    <button class="btn btn-sm btn-success">Hold</button>
                </div>*@


            <div class="tab-pane fade show active" id="ringList" role="tabpanel" aria-labelledby="rings-tab">
                <div class="row">
                    <div class="col-12">
                        <div class="">
                            <form method="post" asp-action="SaveAllProduct" asp-controller="Jewellery">
                                <div class="container mt-2">
                                    <div class="row">
                                        <div class="col-xxl-12 col-xl-12 col-lg-12 col-md-12 col-sm-12 offset-xxl-5 offset-xl-5 offset-lg-4 offset-md-4 ">
                                            <button class="btn btn-theme" id="btnVarify">Next</button>
                                        </div>
                                    </div>

                                </div>

                                <table class="table" id="ringTable">
                                    <thead>
                                        <tr>
                                            <th scope="col">#</th>
                                            <th scope="col">
                                                Title
                                            </th>
                                            <th scope="col">
                                                Vender
                                            </th>
                                            <th scope="col">Vendor Style</th>
                                            <th scope="col">Length</th>
                                            <th scope="col">Band Width</th>
                                            <th scope="col">Weight</th>
                                            <th scope="col">CTV</th>
                                            <th scope="col">Center Shape</th>
                                            <th scope="col">Center Carat</th>
                                            <th scope="col">Certification</th>
                                            <th scope="col">Metal</th>
                                            <th scope="col">Accent Stone Shape</th>
                                            <th scope="col">MM Size</th>
                                            <th scope="col">Dia Wt</th>
                                            <th scope="col">No Of Stones</th>
                                            <th scope="col">Grades</th>
                                            <th scope="col">Diamond Type</th>
                                            <th scope="col">Price</th>
                                            <th scope="col">Wholesale Cost</th>
                                            <th scope="col">Description</th>

                                        </tr>
                                    </thead>
                                    <tbody>
                                        @if (Model != null)
                                        {
                                            @foreach (var item in Model.Where(x => x.CategoryName == SD.Rings))
                                            {

                                                <tr>
                                                    <td scope="col">@(x++)</td>
                                                    <td scope="col">
                                                        <input type="text" asp-for="@item.Title" value="@item.Title" style="width:200px;" />
                                                    </td>
                                                    <td scope="col"><input type="text" asp-for="@item.VenderName" style="width:50px;" /></td>
                                                    <td scope="col"><input type="text" asp-for="@item.VenderStyle" style="width:90px;" /></td>
                                                    <td scope="col"><input type="text" asp-for="@item.Length" style="width:90px;" /></td>
                                                    <td scope="col"><input type="text" asp-for="@item.BandWidth" style="width:90px;" /></td>
                                                    <td scope="col"><input type="text" asp-for="@item.GoldWeight" style="width:90px;" /></td>
                                                    <td scope="col"><input type="text" asp-for="@item.CTW" style="width:70px;" /></td>
                                                    <td scope="col"><input type="text" asp-for="@item.CenterShapeName" style="width:150px;" /></td>
                                                    <td scope="col"><input type="text" asp-for="@item.CenterCaratName" style="width:70px;" /></td>
                                                    <td scope="col"><input type="text" asp-for="@item.Certificate" style="width:130px;" /></td>
                                                    <td scope="col"><input type="text" asp-for="@item.ColorName" style="width:70px;" /></td>
                                                    <td scope="col"><input type="text" asp-for="@item.AccentStoneShapeName" style="width:120px;" /></td>
                                                    <td scope="col"><input type="text" asp-for="@item.MMSize" style="width:80px;" /></td>
                                                    <td scope="col"><input type="text" asp-for="@item.DiaWT" style="width:70px;" /></td>
                                                    <td scope="col"><input type="text" asp-for="@item.NoOfStones" style="width:70px;" /></td>
                                                    <td scope="col"><input type="text" asp-for="@item.Grades" style="width:70px;" /></td>
                                                    <td scope="col"><input type="text" asp-for="@item.ProductType" style="width:90px;" /></td>
                                                    <td scope="col"><input type="text" asp-for="@item.Price" style="width:100px;" /></td>
                                                    <td scope="col"><input type="text" asp-for="@item.WholesaleCost" style="width:100px;" /> </td>
                                                    <td scope="col"><textarea asp-for="@item.Description" rows="3" style="width:250px;"></textarea></td>
                                                </tr>
                                            }
                                        }
                                        else
                                        {
                                            <tr>
                                                <td colspan="12"> No data found</td>
                                            </tr>
                                        }
                                    </tbody>
                                </table>
                            </form>
                        </div>
                    </div>
                </div>

