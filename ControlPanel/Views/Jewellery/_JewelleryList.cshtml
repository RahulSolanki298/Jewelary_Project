@model IEnumerable<Models.ProductMasterDTO>
@using Common
@{
    var index = 1;
    string defaultImage = string.Empty;
    var ringCount = Model.
           Where(x => x.CategoryName == SD.Rings)
           .GroupBy(p => p.CategoryName)
           .Select(g => g.Count())
           .Sum();

    var pendantCount = Model.
           Where(x => x.CategoryName == SD.Pendants)
           .GroupBy(p => p.CategoryName)
           .Select(g => g.Count())
           .Sum();

    var earringsCount = Model.
           Where(x => x.CategoryName == SD.Earrings)
           .GroupBy(p => p.CategoryName)
           .Select(g => g.Count())
           .Sum();

    var bandsCount = Model.
           Where(x => x.CategoryName == SD.Bands)
           .GroupBy(p => p.CategoryName)
           .Select(g => g.Count())
           .Sum();

    var braceletsCount = Model.
           Where(x => x.CategoryName == SD.Bracelets)
           .GroupBy(p => p.CategoryName)
           .Select(g => g.Count())
           .Sum();
}
<style>
    .card-deck {
        display: flex;
        flex-wrap: nowrap;
        overflow-x: auto;
        gap: 5px; /* Optional: space between images */
        max-width: 250px; /* Limit width, allow scrolling */
    }

        .card-deck .card {
            flex: 0 0 auto;
            border: none;
            background: transparent;
            padding: 0;
        }

        .card-deck img.card-img-top {
            height: 60px; /* Keep height small */
            width: auto;
            object-fit: contain;
            border: 1px solid #ccc;
            border-radius: 4px;
        }

    .image-row {
        display: flex;
        flex-wrap: nowrap;
        overflow-x: auto;
        gap: 5px;
        max-width: 500px; /* Adjust as needed */
    }

    .thumb-img {
        height: 60px;
        width: auto;
        object-fit: contain;
        border: 1px solid #ccc;
        border-radius: 4px;
    }
</style>


<div class="row mb-2">
    <div class="col-lg-12 col-md-10">
        <div class="btn-group dropup float-end">
            <a class="" data-bs-toggle="dropdown" aria-expanded="false">
                <i class="bi bi-three-dots-vertical"></i>
            </a>
            <ul class="dropdown-menu">
                <li><a class="dropdown-item" href="#" data-status="Deactivated">Deactivated</a></li>
                <li><a class="dropdown-item" href="#" data-status="Hold">Hold</a></li>
                <li><a class="dropdown-item" href="#" data-status="Program">Program</a></li>
                <li><a class="dropdown-item" href="#" data-status="Collection">Collection</a></li>
            </ul>
        </div>
    </div>
    <input type="hidden" id="txtProductIds" />
</div>
<section class="section">
