<section class="useful-info-area section-padding bg-light">
	<div class="container">
		<div class="row">
			<div class="col-12 text-center mb-4">
				<h2 class="title">Useful Information About Our Diamonds</h2>
			</div>
		</div>
		<div id="usefulInfoCarousel" class="carousel slide" data-bs-ride="carousel">
			<div class="carousel-inner">
				<!-- Item 1 -->
				<div class="carousel-item active">
					<div class="info-card text-center p-4">
						<h3 class="info-title">High-Quality Diamonds</h3>
						<p class="info-description">Our diamonds are sourced from the most trusted mines, ensuring you get the best quality at competitive prices.</p>
					</div>
				</div>
				<!-- Item 2 -->
				<div class="carousel-item">
					<div class="info-card text-center p-4">
						<h3 class="info-title">Certified Stones</h3>
						<p class="info-description">All our diamonds come with certifications from renowned grading laboratories like GIA and AGS.</p>
					</div>
				</div>
				<!-- Item 3 -->
				<div class="carousel-item">
					<div class="info-card text-center p-4">
						<h3 class="info-title">Customizable Designs</h3>
						<p class="info-description">We offer customization options so you can create the perfect piece of jewelry that reflects your unique style.</p>
					</div>
				</div>
				<!-- Item 4 -->
				<div class="carousel-item">
					<div class="info-card text-center p-4">
						<h3 class="info-title">Fast Delivery</h3>
						<p class="info-description">We offer fast and secure delivery options to ensure that your diamonds arrive safely and on time.</p>
					</div>
				</div>
			</div>
			<!-- Controls -->
			<button class="carousel-control-prev" type="button" data-bs-target="#usefulInfoCarousel" data-bs-slide="prev">
				<span class="carousel-control-prev-icon" aria-hidden="true"></span>
				<span class="visually-hidden">Previous</span>
			</button>
			<button class="carousel-control-next" type="button" data-bs-target="#usefulInfoCarousel" data-bs-slide="next">
				<span class="carousel-control-next-icon" aria-hidden="true"></span>
				<span class="visually-hidden">Next</span>
			</button>
		</div>
	</div>
</section>
